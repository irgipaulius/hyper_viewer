    "author": "John Molakvoæ <skjnldsv@protonmail.com>",
    "contributors": [
        "John Molakvoæ <skjnldsv@protonmail.com>"
    ],
    "keywords": [
        "nextcloud",
        "hyper_viewer",
        "app",
        "dev",
        "vuejs"
    ],
    "bugs": {
        "url": "https://github.com/skjnldsv/hyper_viewer/issues"
    },
    "repository": {
        "url": "https://github.com/skjnldsv/hyper_viewer",
        "type": "git"
    },
    "homepage": "https://github.com/skjnldsv/hyper_viewer",
    "license": "agpl",
    "private": true,
    "scripts": {
